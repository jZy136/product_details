@model IEnumerable<product_details.ViewModel.p_sd>

@{
    ViewBag.Title = "View";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.pd_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ct_num)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.pd_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ct_num)
            </td>
        </tr>
    }

</table>

